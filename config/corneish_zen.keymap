#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/mouse.h>

// Layer aliases
#define QWERTY  0
#define NAV     1
#define MEDIABT 2
#define SYMBOL  3
#define NUMBER  4
#define MOUSE   5
#define FNLAYER 6

// window manager shortcuts
#define WTOPLEFT  LC(LA(U))
#define WTOPRIGHT LC(LA(I))
#define WBTMLEFT  LC(LA(J))
#define WBTMRIGHT LC(LA(K))
#define WNEXTSCRN RC(RA(RG(RIGHT)))
#define WPREVSCRN RC(RA(RG(LEFT)))
// Window thirds
#define WLEFT_THIRD LC(LA(D))
#define WCENTER_THIRD LC(LA(F))
#define WRIGHT_THIRD LC(LA(G))
#define WTOP_THIRD LC(LA(D))  // Using D for consistency
#define WBOTTOM_THIRD LC(LA(G))  // Using G for consistency
// Window halves
#define WLEFT_HALF LC(LA(LEFT))
#define WRIGHT_HALF LC(LA(RIGHT))
#define WTOP_HALF LC(LA(UP))
#define WBOTTOM_HALF LC(LA(DOWN))
// Window two-thirds
#define WLEFT_TWO_THIRDS LC(LA(E))
#define WCENTER_TWO_THIRDS LC(LA(R))
#define WRIGHT_TWO_THIRDS LC(LA(T))
// Window center and fullscreen
#define WCENTER_HALF LC(LA(C))
#define WFULLSCREEN LG(LS(F))  // Cmd+Shift+F for fullscreen
#define WRESTORE LC(LA(BACKSPACE))

// tab navigation shortcuts
#define TAB_RT   LC(TAB)
#define TAB_LT   LC(LS(TAB))
// macOS specific commands
#define FILEMENU LG(LS(SLASH))  // Cmd+Shift+/ for help menu search
#define DOCK     LC(F3)

// media navigation shortcuts
#define FWD10S   LG(LA(RIGHT_ARROW)) 
#define RWD10S   LG(LA(LEFT_ARROW))
#define FWD1M   LG(LS(RIGHT_ARROW)) 
#define RWD1M   LG(LS(LEFT_ARROW))

/ {
  chosen {
    zmk,matrix_transform = &default_transform;
    //zmk,matrix_transform = &five_column_transform;
  };
};

&mmv {
    time-to-max-speed-ms = <1000>;
    acceleration-exponent=<1>;
};

/ {
        behaviors {
                ms: mod_sticky_key {
                compatible = "zmk,behavior-hold-tap";
                #binding-cells = <2>;
                flavor = "hold-preferred";
                tapping-term-ms = <200>;
                bindings = <&kp>, <&sk>;
                };
                ctrlAlt: tap_dance_0 {
                compatible = "zmk,behavior-tap-dance";
                #binding-cells = <0>;
                tapping-term-ms = <200>;
                bindings = <&ms LS(LC(LALT)) RALT>, <&sk RCTL>, <&sk RC(RALT)>;
                };
                shiftCap: tap_dance_1 {
                compatible = "zmk,behavior-tap-dance";
                #binding-cells = <0>;
                tapping-term-ms = <200>;
                bindings = <&sk LSHIFT>, <&kp CAPSLOCK>;
                };
                qweNavMda: tap_dance_2 {
                compatible = "zmk,behavior-tap-dance";
                #binding-cells = <0>;
                tapping-term-ms = <300>;
                bindings = <&to QWERTY>, <&to NAV>, <&to MEDIABT>;
                };
                symNumMse: tap_dance_3 {
                compatible = "zmk,behavior-tap-dance";
                #binding-cells = <0>;
                tapping-term-ms = <300>;
                bindings = <&to SYMBOL>, <&to NUMBER>, <&to MOUSE>;
                };
                // Window positioning tap dances
                wLeft: window_left_dance {
                compatible = "zmk,behavior-tap-dance";
                #binding-cells = <0>;
                tapping-term-ms = <200>;
                bindings = <&kp WLEFT_THIRD>, <&kp WLEFT_HALF>, <&kp WLEFT_TWO_THIRDS>;
                };
                wRight: window_right_dance {
                compatible = "zmk,behavior-tap-dance";
                #binding-cells = <0>;
                tapping-term-ms = <200>;
                bindings = <&kp WRIGHT_THIRD>, <&kp WRIGHT_HALF>, <&kp WRIGHT_TWO_THIRDS>;
                };
                wTop: window_top_dance {
                compatible = "zmk,behavior-tap-dance";
                #binding-cells = <0>;
                tapping-term-ms = <200>;
                bindings = <&kp WTOP_THIRD>, <&kp WTOP_HALF>, <&kp WTOP_THIRD>;
                };
                wBottom: window_bottom_dance {
                compatible = "zmk,behavior-tap-dance";
                #binding-cells = <0>;
                tapping-term-ms = <200>;
                bindings = <&kp WBOTTOM_THIRD>, <&kp WBOTTOM_HALF>, <&kp WBOTTOM_THIRD>;
                };
                wCenter: window_center_dance {
                compatible = "zmk,behavior-tap-dance";
                #binding-cells = <0>;
                tapping-term-ms = <200>;
                bindings = <&kp WCENTER_THIRD>, <&kp WCENTER_HALF>, <&kp WCENTER_TWO_THIRDS>;
                };
                wCenterHold: window_center_hold {
                compatible = "zmk,behavior-hold-tap";
                #binding-cells = <2>;
                flavor = "tap-preferred";
                tapping-term-ms = <200>;
                bindings = <&kp>, <&wCenter>;
                };
                colonFn: colon_fn_hold_tap {
                compatible = "zmk,behavior-hold-tap";
                #binding-cells = <2>;
                flavor = "tap-preferred";
                tapping-term-ms = <200>;
                bindings = <&sk>, <&colonTapDance>;
                };
                colonTapDance: colon_tap_dance {
                compatible = "zmk,behavior-tap-dance";
                #binding-cells = <0>;
                tapping-term-ms = <200>;
                bindings = <&kp COLON>, <&kp LS(F20)>;
                };
                symHoldTap: symbol_hold_tap {
                compatible = "zmk,behavior-hold-tap";
                #binding-cells = <2>;
                flavor = "tap-preferred";
                tapping-term-ms = <200>;
                bindings = <&mo>, <&symNumMse>;
                };
                navHoldTap: nav_hold_tap {
                compatible = "zmk,behavior-hold-tap";
                #binding-cells = <2>;
                flavor = "tap-preferred";
                tapping-term-ms = <200>;
                bindings = <&mo>, <&qweNavMda>;
                };
                mediaCtrl: media_hold_tap {
                compatible = "zmk,behavior-hold-tap";
                #binding-cells = <2>;
                flavor = "tap-preferred";
                tapping-term-ms = <200>;
                bindings = <&kp>, <&kp>;
                };
        };

        keymap {
                compatible = "zmk,keymap";

